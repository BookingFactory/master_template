                    <li>
                      <a href="{{ link.value | link_to: website_data.link_object }}">{{link.title}}</a>
                    </li>
                  {% elsif link.category == 'Blog Record Link' %}
                    <li>
                      <a href="{{ 'blog/' | append: link.value | link_to: website_data.link_object }}">Read more...</a>
